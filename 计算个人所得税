#include<stdio.h>
int main()
{
	int x;
	double t;
	scanf("%d",&x);
	if(x<=3500)
	{
		t=0;
	}
	else if(3500<x&&x<=5000)
	{
		t=0.03*(x-3500);
	}
	else if(5000<x&&x<=8000)
	{
		t=0.1*(x-5000)+1500*0.03;
	}
	else if(8000<x&&x<=12500)
	{
		t=0.2*(x-8000)+3000*0.1+1500*0.03;
	}
	else if(12500<x&&x<=38500)
	{
		t=0.25*(x-12500)+4500*0.2+3000*0.1+1500*0.03;
	}
	else if(38500<x&&x<=58500)
	{
		t=0.3*(x-38500)+26000*0.25+4500*0.2+3000*0.1+1500*0.03;
	}
	else if(58500<x&&x<=83500)
	{
		t=0.35*(x-58500)+20000*0.3+26000*0.25+4500*0.2+3000*0.1+1500*0.03;
	}
	else
	{
		t=0.45*(x-83500)+25000*0.35+20000*0.3+26000*0.25+4500*0.2+3000*0.1+1500*0.03;
	}
	printf("%.2f",t);
	return 0;
}

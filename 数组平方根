#include<stdio.h>
#include<math.h>
int main()
{
	int i,j,n;
	scanf("%d",&n);
	int arr[n];
	float m;
	if(n<=0)
	{
		printf("ERROR\n");
	}
	else
	{ 
		for(j=0;j<n;j++)
		{
			scanf("%d",&arr[j]);
		}
		for(i=0;i<n;i++)
		{
			m=sqrt(arr[i]);
			if(arr[i]<0)
			{
				printf("ERROR\n");
			}
			else
			{
				printf("%.2f\n",m);
			}
		}
	}
	return 0;
}

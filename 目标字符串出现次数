#include<stdio.h>
#include<string.h>
int main()
{
	int n,i,j,m;
	char strarr[10][100];
	char str[100];
	scanf("%d",&n);
	for(i=0;i<n;i++)	
		scanf("%s",&strarr[i]);
	scanf("%s",&str);
	i=0;
	m=0;
	while(i<n)
	{
		if(strcmp(str,strarr[i])==0)
		{
			m++;
		}
		i++;	
	}
	printf("%d\n",m);
	return 0;
}

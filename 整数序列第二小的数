#include<stdio.h>
int secondNumber(int n)
{
	int i,j,k,sec,min,max;			//第二小且非最大 
	int data[n];
	for(i=0;i<n;i++)
	{
		scanf("%d",&data[i]);
	}
	min=data[0];
	max=data[1];
	sec=data[2];
	for(j=1;j<n;j++)				//排出最小及第二小的数 
	{
		if(data[j]<min)					
		{
			sec=min;
			min=data[j];
		}
		else if(data[j]>min)
		{
			if(data[j]<sec)
				sec=data[j];
		}
	}
	for(k=1;k<n;k++)				//确认非最大 
	{
		if(data[k]>max)
		{
			max=data[k];
		}
	}
	if(sec==min||sec==max||n<3)
		return -1;
	else
		return sec;
}
int main()
{
	int i,m,n,sec;
	int arr[m]; 
	scanf("%d",&m);
	for(i=0;i<m;i++)
	{
		scanf("%d",&arr[i]);
		n=arr[i];
		sec=secondNumber(n);
		if(sec==-1)
		{
			printf("ERROR\n");
		}
		else
		{
			printf("%d\n",sec);
		}
	}
	return 0;	
} 
